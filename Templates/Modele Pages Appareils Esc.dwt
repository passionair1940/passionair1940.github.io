  <!-- #BeginEditable "Liste%20appareils" -->
  <div id="Layer2" style="position:absolute; left:45px; top:100px; width:1111px; height:112px; z-index:57"> 
    <table width="100%" border="1">
      <tr bgcolor="#999999"> 
        <td width="14%"> 
          <div align="center"><font size="3"><b>Type</b></font></div>
        </td>
        <td width="5%"> 
          <div align="center"><font size="3"><b>N&deg; de s&eacute;rie</b></font></div>
        </td>
        <td width="6%"> 
